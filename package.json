{
  "name": "hackrank",
  "version": "1.0.0",
  "description": "Solutions to HackerRank problems",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "dev:watch": "npm run dev -- --watch",
    "prod": "webpack --config webpack.prod.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run dev && npm run prod",
    "lint": "eslint src -f html -o reports/lint.html --fix",
    "lint:watch": "esw src --watch",
    "test": "jest",
    "commit": "git add . && git-cz",
    "release": "corp-semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyler-austin/hackerrank.git"
  },
  "keywords": [
    "HackerRank"
  ],
  "author": "Tyler Austin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyler-austin/hackerrank/issues"
  },
  "homepage": "https://github.com/tyler-austin/hackerrank#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-wildcard": "^5.0.0",
    "clean-webpack-plugin": "0.1.19",
    "commitizen": "^3.0.7",
    "corp-semantic-release": "^6.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^4.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "esm": "^3.2.20",
    "expose-loader": "^0.7.5",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prettier-loader": "2.1.1",
    "prettier-webpack-plugin": "1.2.0",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "2.6.3",
    "validate-commit-msg": "^2.14.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,md}": [
      "npm run lint",
      "git add"
    ]
  }
}
